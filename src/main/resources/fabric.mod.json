{
  "schemaVersion": 1,
  "id": "enderspecimina",
  "version": "${version}",
  "name": "Ender Specimina",
  "description": "Client-side utilities and integrations for 1.21.5.",
  "authors": ["enderkill98"],
  "contact": {
    "sources": "https://github.com/your/repo"
  },
  "license": "MIT",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.enderkill98.enderspecimina.EnderSpeciminaClient"
    ],
    "modmenu": [
      "me.enderkill98.enderspecimina.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft":    "~${minecraft_version}",
    "fabric-api":   ">=${fabric_api_version}"
  },
  "suggests": {
    "modmenu": "*",
    "yacl": "*",
    "xaerominimap": "*",
    "xaeroworldmap": "*"
  },
  "custom": {
    "java": "${java_version}",
    "maven_group": "${maven_group}",
    "archives_base_name": "${archives_base_name}"
  },
  "mixins": [
    "enderspecimina.mixins.json",
    "enderspecimina.xaerominimap.mixins.json",
    "enderspecimina.xaeroworldmap.mixins.json"
  ]
}
